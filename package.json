{
  "name": "@condensetech/cdk-lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/condensetech/cdk-lib"
  },
  "version": "0.1.0-rc.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "Condense s.r.l.",
    "email": "tech@condense.tech"
  },
  "scripts": {
    "clean": "tsc -b --clean",
    "build": "jsii",
    "package": "jsii-pacmak",
    "watch": "jsii --watch",
    "test": "jest",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "aws-cdk-lib": "2.149.0",
    "constructs": "10.3.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jsii": "^5.4.26",
    "jsii-pacmak": "^1.101.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.149.0",
    "constructs": "^10.3.0"
  },
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "io.github.condensetech.cdklib",
        "maven": {
          "groupId": "io.github.condensetech",
          "artifactId": "cdklib"
        }
      },
      "python": {
        "distName": "condensetech-cdk-lib",
        "module": "condensetech_cdk_lib"
      },
      "dotnet": {
        "namespace": "Condense.CdkLib",
        "packageId": "Condense.CdkLib"
      }
    },
    "tsc": {
      "outDir": "lib",
      "rootDir": "lib"
    }
  }
}